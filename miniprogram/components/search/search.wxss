/* components/search-bar.wxss */



.search-container{
  margin-top: 5px;
  display:flex;
}
.search-container input{
  border-radius: 3px 0 0 3px;
  background-color: #c7d0d8;
  width: 90%;
  padding:5px;
}

.search-container button{
  width: 23px;
  background-color: #b7b7b8;
  border-radius: 0 3px 3px 0;
  padding:0;
  min-height: 0;
  margin:0;
}

.iconButton{
  margin-top:2px;
}